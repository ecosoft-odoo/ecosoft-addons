<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="seq_wht_option_1" model="ir.sequence">
        <field name="name">Withholding tax cert sequence</field>
        <field name="code">withholding.tax.cert</field>
        <field name="padding" eval="4" />
        <field name="prefix">WHT/%(year)s/</field>
    </record>
    <record id="wht_sequence_option" model="ir.sequence.option">
        <field name="name">WHT Cert.</field>
        <field name="model">withholding.tax.cert</field>
        <field name="use_sequence_option" eval="True" />
    </record>
    <record id="wht_sequence_option_line_1" model="ir.sequence.option.line">
        <field name="base_id" ref="wht_sequence_option" />
        <field name="name">WHT Cert. Option 1</field>
        <field name="filter_domain">[]</field>
        <field
            name="sequence_id"
            ref="l10n_th_account_wht_cert_form_sequence.seq_wht_option_1"
        />
    </record>
</odoo>
